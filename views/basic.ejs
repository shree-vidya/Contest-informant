<!DOCTYPE html>
<html>
    <head>
        <title>Contest informant</title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
        <link rel="stylesheet" href="/stylesheets/basic.css">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js" type="text/javascript" async></script>
    </head>
    <body>
    
    <div class="container">
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>
    
    <div id="landing-header">
         <h1>Welcome to Contest Informant!</h1>
         <h2>Choose your interest </h2>
         <div class="but">
            <button><a href="/coding" class="btn btn-lg btn btn-outline-info btn-block">Coding</a></button>
        </div>
        <div class="but">
            <button><a href="/art" class="btn btn-lg btn btn-outline-dark btn-block">Art And Literature</a></button>
        </div>
        <div class="but">
            <button><a href="/cultural" class="btn btn-lg btn btn-outline-dark btn-block">Dance and music</a></button>
        </div>
        <div class="but">
            <button><a href="/general" class="btn btn-lg btn btn-outline-dark btn-block">General</a></button>
        </div>
        
    </div>
    
    <ul class="slideshow">
      <li></li>
      <li></li>
      <li></li>
      <li></li>
      <li></li>
    </ul>


